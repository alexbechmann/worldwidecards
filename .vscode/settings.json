{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/*.js.map": true,
    "**/*.js": {
        "when": "$(basename).ts"
    },
    "**/*.d.ts": {
        "when": "$(basename).ts"
    },
    "**/dist/**": true
  },
  "editor.tabSize": 2
}
